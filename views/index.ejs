            for(let i=0; i < images.length; i++){ %>
              <div class="main-problem">
                <!-- <% if(imagesText[i] == 'Transtornos Alimenticios') { %>
                  <p class="transtornos">Transtornos <br> Alimenticios</p>
                <% }else{%> -->
                <!-- <% } %> -->
                <img src="images/portadas/<%= images[i] %>/<%= images[i] %>.png" alt="<%= images[i] %>_image">
                <p><%= imagesText[i] %></p>
              </div>
            <% } %>
          </div>
        </section>
